  "main": "lib/src/index.js",
  "dependencies": {
    "@knshiro/firetable-functions": "0.0.8",
    "@types/algoliasearch": "^3.34.5",
    "algoliasearch": "^3.35.1",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.3.0"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "ts-node": "^8.6.2"
  },
  "private": true
}
